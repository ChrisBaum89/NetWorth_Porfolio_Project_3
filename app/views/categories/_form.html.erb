<!-- display errors if object is not saved -->
<%= render "layouts/errors" %>

<!-- new/edit object form -->
<%= form_for(@category) do |f| %>
  <br>Category Name:<br>
  <%= f.text_field :name %><br><br>
  <%=f.hidden_field :dollar_value, value: 0%>
  <%=f.hidden_field :number_of_users, value: 0%>

  <%= f.submit %>
<% end %>
